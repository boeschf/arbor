<?xml version="1.0" encoding="UTF-8"?>
<jube>
    <parameterset name="jobsystem_params">
        <parameter name="submit_cmd">.</parameter>
        <parameter name="submit_cmd_args"></parameter>
        <parameter name="run_cmd"></parameter>
        <parameter name="run_cmd_args"></parameter>
        <parameter name="node_idx">0</parameter>
        <parameter name="max_node_idx">0</parameter>
        <parameter name="nodes" mode="python">[1][$node_idx]</parameter>
    </parameterset>

    <parameterset name="bench_params">
        <parameter name="num_cells" type="int">100,200</parameter>
        <parameter name="ring_size" type="int">10</parameter>
        <parameter name="duration" type="int">10</parameter>
        <parameter name="dt" type="float">0.025</parameter>
        <parameter name="min_delay" type="int">2</parameter>
        <parameter name="num_synapses" type="int">100</parameter>
    </parameterset>
</jube>

